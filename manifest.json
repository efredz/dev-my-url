{
    "manifest_version": 3,
    "name": "dev-my-url",
    "version": "1.0",
    "description": "changes www for .dev and adds default port of 8443",
    "icons": {
        "16": "icons/code-16.png",
        "32": "icons/code-32.png",
        "96": "icons/code-96.png"
    },
    "action": {
        "default_icon": {
            "16": "icons/code-16.png",
            "32": "icons/code-32.png",
            "96": "icons/code-96.png"
        }
    },
    "permissions": [
        "activeTab",
        "tabCapture",
        "storage",
        "tabs"
    ],
    "background": {
        "matches": [
            "<all_urls>"
        ],
        "service_worker": "main.js"
    }
}